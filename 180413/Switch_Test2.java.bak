class Switch_Test2 {
	public static void main(String[] args) {
		char score=78;
			switch(score/10){
				case 9 : System.out.println("A");
				case 8 : System.out.println("B");
				case 7 : System.out.println("C");
				case 6 : System.out.println("D");
				default : System.out.println("잘못입력");
			}
	}
}