class If_Test8 {
	public static void main(String[] args){
		char score=78;
			if(score>=90)
			{
				System.out.println("A");
			}
			else if(score>=80)
			{
				System.out.println("B");
			}
			else if(score>=70)
			{
				System.out.println("C");
			}
			else if(score>=60)
