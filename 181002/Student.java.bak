class Student {
	private String name,grade,ban,telephone;

	public Student(String name){
		this.name=name;
	}
	public Student(String name, String grade, String ban, String telephone){
		this.name=name;
		this.grade=grade;
		this.ban=ban;
		this.telephone=telephone;
	}
	public Student(String name, String ban, String telephone){
		this.name=name;
		this.ban=ban;
		this.telephone=telephone;
	}
	public Student(String grade, String ban, String telephone){
		this.grade=grade;
		this.ban=ban;
		this.telephone=telephone;
	}

	public String study(String subject){
		return subject;
	}
	public static void main(String[] args){
		Student s1=new Student("장민재");
		Student s2=new Student("김현우");
		Student s3=new Student("Tomas","1","1","111");
		Student s4=new Student("John","6","222");
		Student s5=new Student("2","5","333");

		System.out.println(s3.name+"는 "s3.grade+"학년이고 반은 "+s3.ban+", 전화번호는 "+s3.telephone+"입니다.");
		System.out.println(s5.study("자바")+"를 공부했다.");
	}	


}