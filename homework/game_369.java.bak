public class game_369 {
	public static void main(String[] args) {
		
		System.out.println("3 6 9 게임 박수를 쳐야 할 때는?? ");
		for (int i =1;i<=100;i++){
			if (i%10==3||i%10==6||i%10==9){
				if (i/10==3||i/10==6||i/10==9){	
					System.out.println(i+"  짝짝");
				} 
				else {
					System.out.println(i+"  짝");
				}
			}
		}
	}
}